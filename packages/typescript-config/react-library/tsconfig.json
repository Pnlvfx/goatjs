{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "../base/tsconfig.json",
  "display": "React Library",
  "compilerOptions": {
    "incremental": true,
    "target": "ES2022",
    "lib": ["es2022", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "node",
    "importHelpers": true,
    "declarationMap": true,
    "sourceMap": true
  }
}
