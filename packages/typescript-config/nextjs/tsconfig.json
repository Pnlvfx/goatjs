{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "../base/tsconfig.json",
  "display": "NextJS",
  "compilerOptions": {
    "plugins": [{ "name": "next" }],
    "target": "es2022",
    "lib": ["es2022", "DOM", "DOM.Iterable"],
    "module": "esnext",
    "noEmit": true,
    "resolveJsonModule": true,
    "moduleResolution": "bundler",
    "jsx": "preserve"
  }
}
