{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "../base/tsconfig.json",
  "display": "NextJS",
  "compilerOptions": {
    "incremental": true,
    "plugins": [{ "name": "next" }],
    "target": "es2023",
    "lib": ["ES2023", "DOM", "DOM.Iterable"],
    "module": "esnext",
    "noEmit": true,
    "resolveJsonModule": true,
    "moduleResolution": "bundler",
    "jsx": "react-jsx"
  }
}
