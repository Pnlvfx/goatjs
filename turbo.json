{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "tsc": {
      "dependsOn": ["^tsc"],
      "cache": true
    },
    "lint": {
      "dependsOn": ["^lint"],
      "cache": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "cache": true
    }
  }
}
